    "auto-ingest:status": "tsx scripts/auto-ingest.ts status",
    "auto-ingest:run-once": "tsx scripts/auto-ingest.ts run-once",
    "analyze": "ANALYZE=true next build",
    "bundle-report": "npx @next/bundle-analyzer",
    "lighthouse": "npx lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "perf": "npm run build && npm run lighthouse",
    "security-audit": "tsx scripts/security-audit.ts",
    "security-check": "npm run security-audit && npm audit",
    "db-optimize": "tsx scripts/database-optimize.ts",
    "db-analyze": "npx prisma db execute --file scripts/analyze-tables.sql",
    "db-vacuum": "npx prisma db execute --file scripts/vacuum-tables.sql",
    "error-monitor": "tsx scripts/error-monitor.ts",
    "error-check": "npm run error-monitor",
    "analytics-monitor": "tsx scripts/analytics-monitor.ts",
    "analytics-check": "npm run analytics-monitor",
    "mobile-performance": "tsx scripts/mobile-performance.ts",
    "mobile-check": "npm run mobile-performance",
    "accessibility-audit": "tsx scripts/accessibility-audit.ts",
    "accessibility-check": "npm run accessibility-audit",
    "vendor-dashboard": "tsx scripts/vendor-dashboard.ts",
    "vendor-stats": "npm run vendor-dashboard stats",
    "vendor-report": "npm run vendor-dashboard report",
    "vendor-export": "npm run vendor-dashboard export",
    "ml-training": "tsx scripts/ml-training.ts",
    "ml-train": "npm run ml-training train",
    "ml-monitor": "npm run ml-training monitor",
    "ml-report": "npm run ml-training report",
    "ml-update-niches": "npm run ml-training update-niches",
    "ml-cleanup": "npm run ml-training cleanup",
    "cms-automation": "tsx scripts/cms-automation.ts",
    "cms-automate": "npm run cms-automation automation",
    "cms-gift-guides": "npm run cms-automation gift-guides",
    "cms-blog-posts": "npm run cms-automation blog-posts",
    "cms-analytics": "npm run cms-automation analytics",
    "cms-optimize": "npm run cms-automation optimize",
    "cms-report": "npm run cms-automation report",
    "monetization-automation": "tsx scripts/monetization-automation.ts",
    "monetization-automate": "npm run monetization-automation automation",
    "monetization-affiliate": "npm run monetization-automation affiliate",
    "monetization-pricing": "npm run monetization-automation pricing",
    "monetization-ab-tests": "npm run monetization-automation ab-tests",
    "monetization-report": "npm run monetization-automation report",
    "monetization-health": "npm run monetization-automation health",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
