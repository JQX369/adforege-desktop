{
  "name": "presentgogo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "build:tokens": "node scripts/build-tokens.js",
    "typecheck": "tsc --pretty false --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rimraf .next coverage dist",
    "postinstall": "prisma generate",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "vitest run --coverage",
    "test:all": "npm run test && npm run test:e2e",
    "ingest": "tsx scripts/ingest-from-apis.ts --provider=all",
    "ingest:rainforest": "tsx scripts/ingest-from-apis.ts --provider=rainforest",
    "ingest:ebay": "tsx scripts/ingest-from-apis.ts --provider=ebay",
    "ingest:dry-run": "tsx scripts/ingest-from-apis.ts --provider=all --dry-run",
